<h2>プロフィール編集画面</h2>
<%= form_with model: @user do |f| %>
    <label for="inputName">Name</label>
    <%= f.text_field :name, autofocus: true, id:"inputName", placeholder: "name"%><br>
    <label for="inputImage">profile_image</label>
    <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*", id:"inputName" %><br>
    <%= f.submit "保存する" %>
<% end %>
